{
  "routes": [
    {
      "route": "/js/*",
      "serve": "/js/"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/js/*", "/*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".css": "text/css"
  }
}